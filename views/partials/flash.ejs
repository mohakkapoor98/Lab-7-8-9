<% if (flash) { %>
    <!-- on each iteration it will return the key and the array of flash messages -->
    <% for(let [alertType, messages] of Object.entries(flash)) {
        %>
        <div class="alert alert-<%= alertType %>">
            <% for(let message of messages){ %>
            <%= message %><br>
            <% } %>
        </div>
        <% } %>
<% } %>